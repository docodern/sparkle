{
  "id": "page",
  "label": "Page",
  "format": "page",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": { "label": "UID", "placeholder": "URL slug for the page" }
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "placeholder": "Title for the page",
          "allowTargetBlank": false,
          "single": "heading1"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "hero": { "type": "SharedSlice" },
            "image": { "type": "SharedSlice" },
            "text_with_features": { "type": "SharedSlice" },
            "text_with_image": { "type": "SharedSlice" },
            "video_bloks": { "type": "SharedSlice" },
            "text_block": { "type": "SharedSlice" },
            "gallery_block": { "type": "SharedSlice" },
            "cta_block": { "type": "SharedSlice" },
            "contact_form": { "type": "SharedSlice" },
            "supporters": { "type": "SharedSlice" },
            "image_with_text": { "type": "SharedSlice" },
            "testimonials": { "type": "SharedSlice" },
            "features": { "type": "SharedSlice" },
            "text_image_on_background": { "type": "SharedSlice" }
          }
        }
      }
    }
  }
}
